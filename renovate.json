{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "github>halkeye/halkeye//.github/renovate/base.json5"
  ],
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": [
        "^Dockerfile$"
      ],
      "matchStrings": [
        "https://github.com/(?<depName>.*?)/releases/(?<currentValue>.*?)/"
      ],
      "datasourceTemplate": "github-releases"
    }
  ]
}
